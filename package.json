{
  "name": "ui-assets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push && next build && npx prisma db seed",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "format": "eslint src --ext .js,jsx,.ts,.tsx --fix && prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "test": "vitest"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "dayjs": "^1.11.18",
    "nanoid": "^5.1.6",
    "next": "^15.5.3",
    "react": "^19",
    "react-dom": "^19",
    "react-intersection-observer": "^9.16.0",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@mswjs/data": "^0.16.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-testing-library": "^7.9.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prisma": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
